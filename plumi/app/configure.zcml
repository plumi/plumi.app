<configure
    xmlns="http://namespaces.zope.org/genericsetup"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:zope="http://namespaces.zope.org/zope"
    i18n_domain="plumi">

  <!-- Generic Setup Profile -->
  <registerProfile
        name="default"
        title="Plumi application setup"
        directory="profiles/default"
        description="The default settings of Plumi portals"
        provides="Products.GenericSetup.interfaces.EXTENSION"
        />

  <five:registerPackage package="." initialize=".initialize" />

  <include package=".policy" />

  <include package="plumi.skin" />
  <include package="plumi.content" />

</configure>

